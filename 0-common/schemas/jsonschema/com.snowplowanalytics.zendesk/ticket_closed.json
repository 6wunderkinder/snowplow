{
	"$schema": "http://json-schema.org/schema#",
	"description": "Schema for a ticket being closed",
	"self": {
		"vendor": "com.snowplowanalytics.zendesk",
		"name": "ticket_closed",
		"format": "jsonschema",
		"version": "1-0-0"
	},

	"type": "object",
	"properties": {
		"account": {
			"type": "string"
		},
		"ccNames": {
			"type": "string"
		},
		"createdAt": {
			"type": "string"
		},
		"createdAtWithTimestamp": {
			"type": "string"
		},
		"dueDate": {
			"type": "string"
		},
		"dueDateWithTimestamp": {
			"type": "string"
		},
		"externalId": {
			"type": "string"
		},
		"id": {
			"type": "string"
		},
		"inBusinessHours": {
			"type": "boolean"
		},
		"priority": {
			"enum": ["Low", "Normal", "High", "Urgent"]
		},
		"score": {
			"type": "number"
		},
		"status": {
			"enum": ["New", "Open", "Pending", "Solved"]
		},
		"tags": {
			"type": "string"
		},
		"ticketType": {
			"type": "string"
		},
		"title": {
			"type": "string"
		},
		"updatedAt": {
			"type": "string"
		},
		"updatedAtWithTimestamp": {
			"type": "string"
		},
		"url": {
			"type": "string",
		},
		"urlWithProtocol": {
			"type": "string",
		},
		"via": {
			"type": "string"
		}
	},
	"minProperties": 1,
	"additionalProperties": false
}
